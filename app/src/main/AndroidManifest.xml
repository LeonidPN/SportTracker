<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sporttracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Views.PreferencesActivity"></activity>
        <activity android:name=".Views.AboutActivity" />
        <activity android:name=".Views.ExerciseRecordsActivity" />
        <activity android:name=".Views.GoalsActivity" />
        <activity android:name=".Views.ProfileActivity" />
        <activity android:name=".Views.AddExerciseRecordActivity" />
        <activity
            android:name=".Views.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>